<ul>
	<% Individual.roots.each do |root| %>
		<div class=<%= root.name %>>
			<% root.children.each do |influence| %>
				<div class=<%=influence.name%>> 
					<strong>Name: <%= influence.name %></strong>, 
					<li> Occupation: <i><%= influence.occupation %>,</li> 
					<li> Ancestry: <%= influence.ancestry %></li> 
					<li> Parent Name: <%= influence.parent.name %></li></i><br>
				</div>
			<% end %>
		</div>
	<% end %>
</ul>
